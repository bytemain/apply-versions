{
  "name": "apply-versions",
  "version": "1.8.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "apply-versions": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "check": "biome check --write",
    "dev": "rslib build --watch",
    "format": "biome format --write",
    "test": "vitest run",
    "apply": "npm run build && node ./dist/cli.js"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.2",
    "@rslib/core": "^0.17.1",
    "@types/node": "^24.10.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.6"
  },
  "dependencies": {
    "@types/prompts": "^2.4.9",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.2",
    "prompts": "^2.4.2",
    "simple-git": "^3.30.0",
    "smol-toml": "^1.4.2"
  }
}
